---

<section
  id="skills"
  class="relative min-h-screen bg-black py-24 px-8 md:px-24 font-['Inter'] overflow-hidden"
>
  <!-- Background effects -->
  <div class="absolute inset-0 -z-10">
    <div
      class="absolute top-1/3 right-1/4 w-96 h-96 bg-purple-600/10 rounded-full blur-3xl"
    >
    </div>
    <div
      class="absolute bottom-1/3 left-1/4 w-80 h-80 bg-indigo-600/10 rounded-full blur-3xl"
    >
    </div>
  </div>

  <div class="max-w-6xl mx-auto">
    <!-- Header -->
    <div class="mb-16">
      <span
        data-skills-animate
        class="inline-block text-xs tracking-[0.3em] uppercase text-purple-400 mb-4 opacity-0"
      >
        What I Work With
      </span>
      <h2
        data-skills-animate
        class="text-4xl md:text-5xl lg:text-6xl font-black text-white opacity-0"
      >
        My
        <span
          class="bg-linear-to-r from-indigo-400 via-purple-500 to-pink-500 bg-clip-text text-transparent"
        >
          Skills
        </span>
      </h2>
    </div>

    <!-- Skills by Category -->
    <div class="space-y-12">
      {
        skillCategories.map((category) => (
          <div>
            <h3
              data-skills-animate
              class="text-lg font-semibold text-purple-400 mb-6 uppercase tracking-widest opacity-0"
            >
              {category.title}
            </h3>
            <div class="grid grid-cols-2 sm:grid-cols-3 md:grid-cols-4 lg:grid-cols-6 gap-4">
              {category.skills.map((skill) => (
                <div
                  data-skill-card
                  class="skill-card group relative p-4 rounded-2xl border border-white/10 bg-white/5 backdrop-blur-sm overflow-hidden hover:border-purple-500/50 transition-all duration-300 cursor-default opacity-0"
                >
                  {/* Glow effect on hover */}
                  <div
                    class="absolute inset-0 opacity-0 group-hover:opacity-20 transition-opacity duration-500 pointer-events-none"
                    style={`background: radial-gradient(circle at center, ${skill.color} 0%, transparent 70%);`}
                  />

                  {/* Icon */}
                  <div
                    class="relative w-10 h-10 mb-3 text-white/60 group-hover:text-white transition-colors duration-300"
                    style={`color: ${skill.color};`}
                    set:html={skill.icon}
                  />

                  {/* Skill name */}
                  <h4 class="relative text-sm font-medium text-white/80 group-hover:text-white transition-colors duration-300">
                    {skill.name}
                  </h4>

                  {/* Subtle bottom accent */}
                  <div
                    class="absolute bottom-0 left-0 right-0 h-0.5 opacity-0 group-hover:opacity-100 transition-opacity duration-500"
                    style={`background: linear-gradient(90deg, transparent, ${skill.color}, transparent);`}
                  />
                </div>
              ))}
            </div>
          </div>
        ))
      }
    </div>
  </div>
</section>

<script>
  import gsap from "gsap";
  import { ScrollTrigger } from "gsap/ScrollTrigger";

  gsap.registerPlugin(ScrollTrigger);

  const initSkillsAnimations = () => {
    const skillsSection = document.getElementById("skills");
    const skillElements = document.querySelectorAll("[data-skills-animate]");
    const skillCards = document.querySelectorAll("[data-skill-card]");

    if (!skillsSection) return;

    // Animate header elements
    if (skillElements.length > 0) {
      gsap.fromTo(
        skillElements,
        {
          opacity: 0,
          y: 30,
          filter: "blur(8px)",
        },
        {
          opacity: 1,
          y: 0,
          filter: "blur(0px)",
          duration: 0.8,
          stagger: 0.15,
          ease: "power3.out",
          scrollTrigger: {
            trigger: skillsSection,
            start: "top 80%",
          },
        },
      );
    }

    // Animate skill cards with stagger
    if (skillCards.length > 0) {
      gsap.fromTo(
        skillCards,
        {
          opacity: 0,
          y: 50,
          scale: 0.9,
        },
        {
          opacity: 1,
          y: 0,
          scale: 1,
          duration: 0.8,
          stagger: 0.1,
          ease: "power4.out",
          scrollTrigger: {
            trigger: skillsSection,
            start: "top 70%",
          },
        },
      );
    }
  };

  // Initialize when DOM is ready
  if (document.readyState === "loading") {
    document.addEventListener("DOMContentLoaded", initSkillsAnimations);
  } else {
    initSkillsAnimations();
  }
</script>

<style>
  .skill-card {
    transform-origin: center;
  }

  .skill-card:hover {
    transform: translateY(-4px);
  }
</style>
